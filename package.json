{
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "start": "pnpm install --shamefully-hoist",
    "dev:demo": "pnpm --filter=ossa-demo dev:h5",
    "build:demo": "pnpm --filter=ossa-demo build:h5 -- --public_path=$npm_config_path",
    "build": "pnpm --filter=ossaui build",
    "clear": "rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/",
    "test": "pnpm --filter=ossa-demo test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint"
    ]
  },
  "author": "",
  "private": true,
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.5",
    "@changesets/cli": "^2.23.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "chalk": "^4.0.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.18.0",
    "eslint-config-taro": "^3.4.12",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^10.1.2",
    "prettier": "^2.7.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "typescript": "^4.7.4"
  }
}
